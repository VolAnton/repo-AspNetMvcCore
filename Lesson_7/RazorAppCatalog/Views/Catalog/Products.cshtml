@model Catalog

@{
    ViewData["Title"] = "Список товаров из каталога";
}

<h1>Всего в каталоге @Model.Count товар(ов)</h1>

<h1>Все товары</h1>
<table>
<tr>
    <th>Id</th>    
    <th>Name</th>
</tr>
 @foreach (var svc in Model.GetAll())
                {
                   <tr>
                       <td>@svc.Id</td>                       
                       <td>@svc.Name</td>
                   </tr>
                }
 </table>
                